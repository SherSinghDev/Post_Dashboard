			})

			$(document).on('submit', '#tracking-update', function (e) {
				e.preventDefault()
				let form = $(this)
				let input = this.trackingid
				let url = form.attr('data-url')
				let data = {
					trackingId: input.value
				}

				$.ajax({
					url,
					data,
